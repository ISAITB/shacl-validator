  rdfs:label "Continent restriction" ;
  rdfs:subClassOf rdfs:Resource ;
  owl:versionInfo "1" ;
  sh:property [
      sh:path skos:inScheme ;
      sh:class skos:ConceptScheme ;
      sh:hasValue <http://publications.europa.eu/resource/authority/continent> ;
      sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
    ] ;
